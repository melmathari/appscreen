{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-utils/schema/capability.json",
  "identifier": "default",
  "description": "Default capabilities for yuzu.shot",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "opener:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        { "path": "**/*" }
      ]
    }
  ]
}
